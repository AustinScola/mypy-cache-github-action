{
  "name": "mypy-cache-github-action",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub Action for caching a mypy cache.",
  "scripts": {
    "build": "tsc --noEmit",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -o dist/restore src/restore.ts && ncc build -o dist/save src/save.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AustinScola/mypy-cache-github-action.git"
  },
  "author": "Austin Scola",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.4.0",
    "@actions/cache": "1.0.7",
    "@actions/github": "5.0.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "15.12.2",
    "@typescript-eslint/parser": "4.27.0",
    "@vercel/ncc": "0.25.1",
    "eslint": "7.28.0",
    "eslint-plugin-github": "4.1.3",
    "eslint-plugin-jest": "24.3.6",
    "jest": "26.6.3",
    "jest-circus": "27.0.4",
    "js-yaml": "4.1.0",
    "prettier": "2.3.1",
    "ts-jest": "26.4.4",
    "typescript": "4.3.2"
  }
}
